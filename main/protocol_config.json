{
  "protocols": [
    {
      "protocol": "uart",
      "inputs": ["clk", "rx"],
      "outputs": ["tx", "tx_out"],
      "parameters": ["baud_rate", "clk_frequency"]
    },
    {
      "protocol": "spi",
      "inputs": ["clk", "miso"],
      "outputs": ["mosi", "cs","sclk"],
      "parameters": ["clk_frequency"]
    },
    {
      "pro" : "i2c",
      "inputs": ["scl"],
      "outputs": ["scl_out"],
      "inout":["sda"],
      "parameters": ["addr","clk_frequency"]
    }
  ]
}
