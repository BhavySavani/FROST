{
  "protocols": [
    {
      "protocol": "uart",
      "inputs": ["clk", "rx"],
      "outputs": ["tx", "tx_out"],
      "parameters": ["baud_rate", "clk_frequency"]
    },
    {
      "protocol": "spi",
      "inputs": ["clk", "miso"],
      "outputs": ["mosi", "cs","sclk"],
      "parameters": ["clk_frequency"]
    },
    {
      "protocol" : "i2c",
      "inputs": ["clk"],
      "outputs": ["scl"],
      "inout":["sda"],
      "parameters": ["addr","clk_frequency"]
    },
    {
      "protocol":"RS485",
      "inputs":["clk","rx"],
      "outputs":["tx"],
      "parameters":["clk_frequency"]
      }
  ]
}
